<body>
	<div id="box">
		
	</div>
	
</body>
<script src="./jquery.js"></script>
<script>
	var no=location.search.split('=')[1]
	$.ajax({
		url:"/index",
		data:{no:no},
		type:"post",
		success:function(msg){
			msg.forEach(function(i){	
				var set="";
				set+="<ul><li>姓名:"+i.name+"</li>"
				+"<li>性别："+i.age+"</li>"
				+"<li>年龄："+i.sex+"</li>"
				+"<li>职业："+i.work+"</li>"
				+"<li>喜好："+i.like+"</li>"
				+"</ul>"
				$("#box").html(set)
			})
		}
	})
	
		
	
	
</script>
